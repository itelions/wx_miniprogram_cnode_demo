<!--components/dialog/dialog.wxml-->
<view class="{{visible?'dialog-main':'dialog-main hidden'}}" bindtap="dialogClose" >
  <view class="dialog-pannel" catchtap>
    <view class="dialog-title">{{title}}</view>
    <scroll-view scroll-y class="dialog-content">
      <slot></slot>  
    </scroll-view>
  </view>
</view>
